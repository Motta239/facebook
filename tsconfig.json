{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/Feed.js",
    "components/Feed.js",
    "components/Header.js",
    "components/Stories.js",
    "pages/api/hello.jsx",
    "pages/index.jsx",
    "components/Story.jsx",
    "components/Sotry/Stories.jsx",
    "components/Header.jsx",
    "components/Feed.jsx"
  ],
  "exclude": ["node_modules"]
}
